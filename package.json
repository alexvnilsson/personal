{
  "name": "alexvnilsson",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexvnilsson.se",
  "scripts": {
    "sass:build": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "sass:watch": "npm run-script sass:build && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "start": "npm-run-all -p sass:watch react:start",
    "build": "npm-run-all sass:build react:build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run-script build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "axios": "^0.20.0",
    "bootstrap": "^4.3.1",
    "camel-case": "^4.1.1",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "graphql": "^15.3.0",
    "invert-color": "^2.0.0",
    "jquery": "^3.5.1",
    "markdown-to-jsx": "^7.0.1",
    "pluralize": "^8.0.0",
    "popper.js": "^1.16.1",
    "react": "^16.14.0",
    "react-dom": "^16.4.1",
    "react-pose": "^4.0.8",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^1.2.1",
    "react-scripts": "^3.4.3",
    "react-transition-group": "^4.2.0",
    "redux": "^4.0.0",
    "restful-react": "^15.1.1",
    "rollup": "^2.9.1"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "^2.13.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
