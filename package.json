{
  "name": "alexvnilsson",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexvnilsson.se",
  "scripts": {
    "sass:build": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "sass:watch": "npm run-script sass:build && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "start": "npm-run-all -p sass:watch react:start",
    "build": "npm-run-all sass:build react:build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run-script build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "chroma-js": "^2.0.4",
    "classnames": "^2.2.6",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "react": "^16.14.0",
    "react-dom": "^16.4.1",
    "react-pose": "^4.0.8",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^1.2.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^4.2.0",
    "redux": "^4.0.0",
    "rollup": "^2.9.1"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "^2.13.8"
  }
}
